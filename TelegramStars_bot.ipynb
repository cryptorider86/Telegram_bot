    {
      "cell_type": "code",
      "source": [
        "# –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∏ –ø—Ä–∏–º–µ–Ω—è–µ–º nest_asyncio\n",
        "!pip install nest_asyncio\n",
        "import nest_asyncio\n",
        "nest_asyncio.apply()\n",
        "\n",
        "import asyncio\n",
        "from telegram import Update, InlineKeyboardButton, InlineKeyboardMarkup\n",
        "from telegram.ext import Application, CommandHandler, MessageHandler, CallbackQueryHandler, filters, ContextTypes\n",
        "\n",
        "# ID Token\n",
        "TELEGRAM_BOT_TOKEN = '8075882848:AAFB_tf6kw-oJfp-WWJ53zXCQgNn3LCMYR8'\n",
        "\n",
        "# ID –∫–∞–Ω–∞–ª–∞\n",
        "CHANNEL_ID = '5632340145'\n",
        "\n",
        "# –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –∫–æ–º–∞–Ω–¥—ã /start\n",
        "async def start(update: Update, context: ContextTypes.DEFAULT_TYPE) -> None:\n",
        "    # –°–æ–∑–¥–∞–ª –∏–Ω–ª–∞–π–Ω-–∫–Ω–æ–ø–∫–∏ –¥–ª—è –≤—ã–±–æ—Ä–∞ —Å—É–º–º—ã\n",
        "    keyboard = [\n",
        "        [\n",
        "            InlineKeyboardButton(\"50 XTR\", callback_data='pay_50'),\n",
        "            InlineKeyboardButton(\"100 XTR\", callback_data='pay_100'),\n",
        "            InlineKeyboardButton(\"150 XTR\", callback_data='pay_150')\n",
        "        ]\n",
        "    ]\n",
        "    reply_markup = InlineKeyboardMarkup(keyboard)\n",
        "    await update.message.reply_text(\n",
        "        '–ü—Ä–∏–≤–µ—Ç! –•–æ—á–µ—à—å –ø–æ–¥–¥–µ—Ä–∂–∞—Ç—å –º–µ–Ω—è? –í—ã–±–µ—Ä–∏ —Å—É–º–º—É –ø–æ–¥–¥–µ—Ä–∂–∫–∏ –≤ Telegram Stars:',\n",
        "        reply_markup=reply_markup\n",
        "    )\n",
        "\n",
        "# –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –Ω–∞–∂–∞—Ç–∏—è –Ω–∞ –∫–Ω–æ–ø–∫—É\n",
        "async def button(update: Update, context: ContextTypes.DEFAULT_TYPE) -> None:\n",
        "    query = update.callback_query\n",
        "    await query.answer()\n",
        "    if query.data == 'pay_50':\n",
        "        context.user_data['amount'] = 50\n",
        "        await send_invoice(update, context, 50)\n",
        "    elif query.data == 'pay_100':\n",
        "        context.user_data['amount'] = 100\n",
        "        await send_invoice(update, context, 100)\n",
        "    elif query.data == 'pay_150':\n",
        "        context.user_data['amount'] = 150\n",
        "        await send_invoice(update, context, 150)\n",
        "\n",
        "# –§—É–Ω–∫—Ü–∏—è –æ—Ç–ø—Ä–∞–≤–∫–∏ –∏–Ω–≤–æ–π—Å–∞\n",
        "async def send_invoice(update: Update, context: ContextTypes.DEFAULT_TYPE, amount: int) -> None:\n",
        "    await context.bot.send_invoice(\n",
        "        chat_id=update.effective_chat.id,\n",
        "        title=\"–ü–æ–¥–¥–µ—Ä–∂–∫–∞ –ü—Å–∏—Ö–æ–ª–æ–≥–∞ –≤ —à–ª—è–ø–µ\",\n",
        "        description=f\"–°–ø–∞—Å–∏–±–æ –∑–∞ –ø–æ–¥–¥–µ—Ä–∂–∫—É! –î–æ–Ω–∞—Ç {amount} Telegram Stars.\",\n",
        "        payload=f\"support_{amount}_stars\",\n",
        "        provider_token=\"\",  # –í–æ—Ç –∑–¥–µ—Å—å –Ω–∞–¥–æ —É–∫–∞–∑–∞—Ç—å –Ω–æ–º–µ—Ä –ø—Ä–æ–≤–∞–π–¥–µ—Ä–∞ –ø–ª–∞—Ç–µ–∂–µ–π, –∫–æ—Ç–æ—Ä—ã–π –Ω–∞–¥–æ –ø–æ–ª—É—á–∏—Ç—å —á–µ—Ä–µ–∑ @BotFather\n",
        "        currency=\"XTR\",  # Telegram Stars –∏–ª–∏ –º–æ–∂–Ω–æ —ç–º–æ–¥–∑–∏ –≤—Å—Ç–∞–≤–∏—Ç—å\n",
        "        prices=[{\"label\": \"–î–æ–Ω–∞—Ç\", \"amount\": amount}],\n",
        "        start_parameter=\"pay\"\n",
        "    )\n",
        "\n",
        "# –û–±—Ä–∞–±–æ—Ç—á–∏–∫ —É—Å–ø–µ—à–Ω–æ–π –æ–ø–ª–∞—Ç—ã\n",
        "async def successful_payment(update: Update, context: ContextTypes.DEFAULT_TYPE) -> None:\n",
        "    payment = update.message.successful_payment\n",
        "    amount = payment.total_amount\n",
        "    await update.message.reply_text(f'–°–ø–∞—Å–∏–±–æ –∑–∞ –¥–æ–Ω–∞—Ç {amount} Stars! –¢—ã –º–æ–ª–æ–¥–µ—Ü!')\n",
        "    await context.bot.send_message(chat_id=CHANNEL_ID, text=f'üéâ –ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å {update.message.from_user.username} –ø–æ–¥–¥–µ—Ä–∂–∞–ª –±–æ—Ç–∞ –Ω–∞ {amount} Stars!')\n",
        "\n",
        "# –û–±—Ä–∞–±–æ—Ç—á–∏–∫ —Ç–µ–∫—Å—Ç–æ–≤—ã—Ö —Å–æ–æ–±—â–µ–Ω–∏–π\n",
        "async def handle_message(update: Update, context: ContextTypes.DEFAULT_TYPE) -> None:\n",
        "    await update.message.reply_text('–°–ø–∞—Å–∏–±–æ –∑–∞ —Å–æ–æ–±—â–µ–Ω–∏–µ! –ò—Å–ø–æ–ª—å–∑—É–π /start –¥–ª—è –Ω–∞—á–∞–ª–∞.')\n",
        "\n",
        "async def main() -> None:\n",
        "    # –°–æ–∑–¥–∞–µ–º –ø—Ä–∏–ª–æ–∂–µ–Ω–∏–µ –±–æ—Ç–∞\n",
        "    application = Application.builder().token(TELEGRAM_BOT_TOKEN).build()\n",
        "\n",
        "    # –†–µ–≥–∏—Å—Ç—Ä–∏—Ä—É–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏\n",
        "    application.add_handler(CommandHandler(\"start\", start))\n",
        "    application.add_handler(CallbackQueryHandler(button))\n",
        "    application.add_handler(MessageHandler(filters.SUCCESSFUL_PAYMENT, successful_payment))\n",
        "    application.add_handler(MessageHandler(filters.TEXT & ~filters.COMMAND, handle_message))\n",
        "\n",
        "    # –ó–∞–ø—É—Å–∫–∞–µ–º –±–æ—Ç–∞ –≤ —Ç–µ–∫—É—â–µ–º —Ü–∏–∫–ª–µ —Å–æ–±—ã—Ç–∏–π\n",
        "    await application.initialize()\n",
        "    await application.start()\n",
        "    await application.updater.start_polling()\n",
        "    print(\"–ë–æ—Ç –∑–∞–ø—É—â–µ–Ω...\")\n",
        "\n",
        "# –ó–∞–ø—É—Å–∫–∞–µ–º –≤ —Ç–µ–∫—É—â–µ–º —Ü–∏–∫–ª–µ —Å–æ–±—ã—Ç–∏–π\n",
        "if __name__ == '__main__':\n",
        "    loop = asyncio.get_event_loop()\n",
        "    loop.create_task(main())\n",
        "    loop.run_forever()"
      ],
